<ol id="activities">
  {{#each model.activities}}
    <li class="year">
      <p class="header">
        <span class="line"></span>
        <span class="text">{{year}}</span>
        <!--<span class="text">No ano {{year}} gastei <span class="value">R$ {{total}}</span>-->
      </p>
    <ol class="month">
      {{#each month}}
      <li>
        <p class="header">
          <span class="line"></span>
          <span class="text">{{hummanize_month}}</span>
          <!--<span class="text">Em {{hummanize_month}} gastei <span class="value">R$ {{total}}</span>-->
        </p>
        <ol>
          {{#each data}}
          <li class="{{type}}">
          <header class="clearfix">
          <div class="img">
            <img {{bind-attr src=subject.image}} alt />
          </div>
          <div class="about">
            <h5 class="ellipsis"><a href="{{subject.url}}">{{subject.name}}</a>{{#in}} <i class="icon-caret-right"></i> <a href="{{url}}" >{{name}}</a>{{/in}}</h5>
            <time>{{post_time}}</time>
          </div>
          </header>
          <p>{{verb}} <span class="value">{{value}}</span>{{preposition}}<span class="object">{{object}}</span>
          {{#content}}<p class="content">{{this}}</p>{{/content}}
          {{#ifvalue type 'video'}}
            <video id="{{id}}" class="video-js vjs-default-skin"
              controls preload="auto" width="640" height="264"
              poster=""
              data-setup=''>
              <source src="{{video_url}}" type='video/mp4' />
              </video>
          {{/ifvalue}}
          <span class="at"> — em <a href="{{location.url}}">{{location.title}}</a></span></p>
          <div class="fb-comments" data-href="http://localhost:3000/nelson-marquezelli/posts/{{id}}" data-width="486" data-numposts="5" data-colorscheme="light"></div>
          </li>
          {{/each}}
        </ol>
      </li>
      {{/each}}
    </ol>
    </li>
  {{/each}}
